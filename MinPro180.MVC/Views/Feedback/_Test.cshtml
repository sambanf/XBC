@model MinPro180.ViewModel.FeedbackViewModel

@{
    ViewBag.Title = "_Test";
}

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.test_id, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownListFor(model => model.test_id, (IEnumerable<SelectListItem>)ViewBag.Testlist, "-- Test --", new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.test_id, "", new { @class = "text-danger" })
            </div>
        </div>
    </div>
}
